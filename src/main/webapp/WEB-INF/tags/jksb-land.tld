  	<body-content>JSP</body-content>
  	<description>数据字典标签</description>
  	<attribute>
  		<name>dictCode</name>
  		<required>false</required>
      	<rtexprvalue>true</rtexprvalue>
  	</attribute>
  	<attribute>
  		<name>dictValue</name>
  		<required>false</required>
      	<rtexprvalue>true</rtexprvalue>
  	</attribute>
  	<attribute>
  		<name>groupId</name>
  		<required>true</required>
      	<rtexprvalue>true</rtexprvalue>
  	</attribute>
  	<attribute>
  		<name>cssClass</name>
  		<required>false</required>
      	<rtexprvalue>true</rtexprvalue>
  	</attribute>
  	<attribute>
  		<name>style</name>
  		<required>false</required>
      	<rtexprvalue>true</rtexprvalue>
  	</attribute>
  	<attribute>
  		<name>id</name>
  		<required>false</required>
      	<rtexprvalue>true</rtexprvalue>
  	</attribute>
  	<attribute>
  		<name>name</name>
  		<required>true</required>
      	<rtexprvalue>true</rtexprvalue>
