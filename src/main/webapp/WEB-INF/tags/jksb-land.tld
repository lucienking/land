  	<attribute>
  		<name>fileTypeField</name>
  		<required>false</required>
      	<rtexprvalue>true</rtexprvalue>
  	</attribute>
  	<attribute>
  		<name>fileNameField</name>
  		<required>false</required>
      	<rtexprvalue>true</rtexprvalue>
