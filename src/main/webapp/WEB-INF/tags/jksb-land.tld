<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE taglib  
	PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"  
	"http://java.sun.com/dtd/web-jsptaglibrary_1_2.dtd"> 
<taglib> 
<tlib-version>1.0</tlib-version> 
<jsp-version>1.2</jsp-version> 
<short-name>jksb-land</short-name> 
<uri>http://www.jksb.com/jksb-land/tags</uri> 
<description>国土管理信息系统自定义TagLib</description> 
  <tag>
    <name>hasAutority</name>
    <tag-class>com.jksb.land.common.tags.HasAuthorityTag</tag-class>
    <body-content>JSP</body-content>
    <description>当前用户拥有权限时才显示标签中的内容</description>
    <attribute>
      <name>authorityId</name>
      <required>true</required>
      <rtexprvalue>true</rtexprvalue>
    </attribute>
  </tag>
  
  <tag>
  	<name>diction</name>
